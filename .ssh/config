Host github.com
  HostName github.com
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_github
  User git
  TCPKeepAlive yes
  IdentitiesOnly yes

Host github.com.private
  HostName github.com
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_github_private
  User git
  TCPKeepAlive yes
  IdentitiesOnly yes
