[init]
  defaultBranch = main
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore_global
[alias]
  st = status
  stt = status -uno
  cm = commit
  difff = diff --name-only
  logg = log --graph --decorate
  g = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h%Creset %cd %Cblue%cn%Creset %s%Cred%d%Creset'
  ss = stash save
  sp = stash pop
  al = config --get-regexp alias.*
  f = fetch -p
  b = branch
  pf = push --force-with-lease
  re = restore
  sw = switch
[pager]
  branch = cat
[pull]
  rebase = false
[merge]
  conflictStyle = diff3
[include]
  path = ~/.gitconfig.local

